<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   android:orientation="vertical"
   android:layout_width="fill_parent"
   android:layout_height="fill_parent"
   android:background="#FFFFFF"
   >
<TextView 
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:textSize="20sp"
   android:text="@string/odblokuj_extendedaccess"
   android:background="@+drawable/buttonColor"
   android:textColor="#FFFFFF"   
   android:padding="10dp"
   />
   <TextView 
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:textColor="#333333"
   android:text="@string/odblokuj_desc"
   android:layout_margin="10dp"
   android:textSize="16sp"
  /> 
   <TextView 
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:textSize="20sp"
   android:id="@+id/log"
   android:background="#000000"
   android:textColor="#FFFFFF"
   android:visibility="gone"
   />
     <Button 
   android:id="@+id/payload_edit_button"
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:text="@string/payload_edit"
   android:visibility="gone"
   />
     <Button 
   android:id="@+id/subscriptions_edit_button"
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:text="@string/subscription_edit"
   android:visibility="gone"
   />
  
   <Spinner
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:id="@+id/item_choices"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
     <Button 
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:text="@string/paywithgoogle"
   android:id="@+id/buy_button"
   
   android:background="@+drawable/rounded_button"  
   android:textColor="#FFFFFF"
   android:padding="10dp"
   android:layout_marginLeft="30dp"
   android:layout_marginRight="30dp"
   android:layout_marginTop="5dp"
   android:layout_marginBottom="5dp"
   />
   <ListView
       android:id="@+id/owned_items"
       android:layout_width="match_parent"
        android:layout_height="wrap_content"
       android:visibility="gone"
   />
   <TextView 
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:textSize="20sp"
   android:layout_marginLeft="10dp"
   android:text="@string/currentmnemoney"
   android:layout_marginTop="5dp"
   android:textColor="@+drawable/buttonColor"   
   />
     <TextView 
   android:id="@+id/current_menmons"
   android:layout_width="fill_parent"
   android:layout_height="wrap_content"
   android:layout_marginTop="5dp"
   android:textSize="16sp"
   android:layout_marginLeft="10dp"
   android:text="@string/nodata"
   android:background="#FFFFFF"
   android:textColor="#000000"   
   />
</LinearLayout>

